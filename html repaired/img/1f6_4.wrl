    Shape	{
		  geometry DEF Ext2 Extrusion {
			  crossSection [
				  .01  .01
					.01 -.01
				 -.01 -.01
				 -.01  .01
				  .01  .01
				]
			}
			appearance Appearance	{
			  material Material	{
				  diffuseColor 1 0 0
				}
			}
		}
		DEF	Builder Script {
			eventOut MFVec3f spine_changed
			eventOut MFVec3f spine1_changed
			eventOut MFVec3f spine2_changed
			directOutput TRUE
			url	"javascript:
			  var n = 100;
				var dx = 2/n;
			  function initialize(){
					for (i=0; i<=n; i++){
						x = .3 + i * dx;
						y = Math.pow(x/2, 1.8);
					  spine_changed[i] = new SFVec3f(x+.2, y+.1, 0);
					} 
					xt=x+.2;yt=y+.1;
					for (i=0; i<=n; i++){
						x = i * dx;
						y = Math.pow(x/2, 1.8);
					  spine1_changed[i] = new SFVec3f(x+xt, y+yt, 0);
					} 
					dx = .5/n;
					for (i=0; i<=n; i++){
						x = i * dx;
						y = 10* Math.pow(x, 2);
					  spine2_changed[i] = new SFVec3f(xt-x, y+yt, 0);
					} 
			  }
			"
#			ROUTE	Builder.spine_changed	TO Ext.set_spine
#			ROUTE	Builder.spine1_changed	TO Ext1.set_spine
			ROUTE	Builder.spine2_changed	TO Ext2.set_spine
		}

		DLine	{
		  startAt	2.5 0 0 
			endAt	2.5 1.38 0
			color	0 0 0
		}

		DLine	{
		  startAt	0 1.38 0 
			endAt	2.5 1.38 0
			color	0 0 0
